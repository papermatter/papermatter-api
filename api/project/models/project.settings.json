{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "name": "project",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "text",
      "required": true
    },
    "location": {
      "type": "string",
      "required": true
    },
    "photos": {
      "type": "component",
      "repeatable": true,
      "component": "page.photo"
    },
    "client": {
      "model": "client"
    },
    "service": {
      "model": "services"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    }
  }
}
