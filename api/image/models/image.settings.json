{
  "kind": "collectionType",
  "collectionName": "images",
  "info": {
    "name": "Image"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true
    },
    "client": {
      "model": "client"
    },
    "service": {
      "model": "services"
    }
  }
}
